<%= semantic_form_for @applicant do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :university %>
    <%= f.input :major %>
    <%= f.input :graduation_time %>
    <%= f.input :sponsorship %>
    <%= f.input :degree %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
